{
    "name": "redking/doctrine-parse-bundle",
    "description": "Doctrine ORM Bundle for Parse.com API",
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/zeliard91/DoctrineParseBundle",
    "keywords": [ "parse", "symfony", "orm", "doctrine", "bundle"],
    "authors": [
        {
            "name": "Damien Matabon",
            "email": "damien.matabon@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "parse/php-sdk": "^1.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/config": "^4.2",
        "symfony/console": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/doctrine-bridge": "^4.2",
        "symfony/yaml": "^4.2",
        "sensio/framework-extra-bundle": "^5.0",
        "doctrine/common": "^2.4",
        "doctrine/instantiator": "^1.2",
        "myclabs/deep-copy": "^1.7"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^4.2",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": { "Redking\\ParseBundle\\": "" }
