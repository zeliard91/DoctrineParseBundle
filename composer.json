{
    "name": "redking/doctrine-parse-bundle",
    "description": "Doctrine ORM Bundle for Parse.com API",
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/zeliard91/DoctrineParseBundle",
    "keywords": [ "parse", "symfony", "orm", "doctrine", "bundle"],
    "authors": [
        {
            "name": "Damien Matabon",
            "email": "damien.matabon@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "parse/php-sdk": "^1.2",
        "symfony/symfony": "^2.8|^3.4",
        "sensio/framework-extra-bundle": "^3.0|^4.0|^5.0",
        "doctrine/common": "^2.4",
        "doctrine/instantiator": "~1.0.1",
        "myclabs/deep-copy": "^1.7",
        "symfony/string": "^5.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": { "Redking\\ParseBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
