{
    "name": "redking/parse-bundle",
    "description": "Doctrine ORM Bundle for Parse.com API",
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://bitbucket.org/redkingteam/redkingparsebundle",
    "keywords": [ "parse", "symfony", "orm", "doctrine", "bundle"],
    "authors": [
        {
            "name": "Damien Matabon",
            "email": "damien.matabon@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "parse/php-sdk": "^1.2",
        "symfony/symfony": "^2.8 || ^3.0",
        "sensio/framework-extra-bundle": "^3.0",
        "doctrine/common": "^2.4",
        "doctrine/instantiator": "~1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": { "Redking\\ParseBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
